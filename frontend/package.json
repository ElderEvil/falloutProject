    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "27.0.0",
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.0",
    "jsdom": "^25.0.1",
    "openapi-typescript": "7.10.1",
    "oxlint": "^1.36.0",
    "tailwindcss": "^4.0.0",
