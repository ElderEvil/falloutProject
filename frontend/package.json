{
  "name": "frontend",
  "version": "2.4.5",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.26.2",
  "scripts": {
    "types:generate": "openapi-typescript http://localhost:8000/api/v1/openapi.json -o src/types/api.generated.ts",
    "dev": "pnpm types:generate && vite",
    "build": "vite build",
    "build:strict": "vue-tsc -b && vite build",
    "build:with-types": "pnpm types:generate && vue-tsc -b && vite build",
    "build:analyze": "ANALYZE=true vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "oxlint src",
    "lint:fix": "oxlint src --fix",
    "typecheck": "vue-tsc --noEmit",
    "ci": "pnpm run lint && pnpm run typecheck && pnpm run test:run"
  },
  "dependencies": {
    "@iconify/vue": "5.0.0",
    "@nuxt/ui": "^4.4.0",
    "@vueuse/core": "14.1.0",
    "@vueuse/motion": "3.0.3",
    "axios": "^1.13.3",
    "jwt-decode": "4.0.0",
    "pinia": "^3.0.4",
    "vue": "^3.5.27",
    "vue-router": "^4.6.4",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@tsconfig/node22": "^22.0.5",
    "@types/jsdom": "27.0.0",
    "@types/node": "^25.0.10",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "jsdom": "^25.0.1",
    "openapi-typescript": "7.10.1",
    "oxlint": "^1.41.0",
    "rollup-plugin-visualizer": "6.0.5",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "npm:rolldown-vite@^7.3.0",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.4"
  }
}
