{
  "name": "frontend",
  "version": "2.10.2",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.28.2",
  "scripts": {
    "types:generate": "openapi-typescript http://localhost:8000/api/v1/openapi.json -o src/core/types/api.generated.ts",
    "dev": "pnpm types:generate && vite",
    "build": "vite build",
    "build:with-types": "pnpm types:generate && vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:e2e": "playwright test --config=tests/e2e/playwright.config.ts",
    "test:e2e:ui": "playwright test --config=tests/e2e/playwright.config.ts --ui",
    "test:e2e:setup": "playwright test --config=tests/e2e/playwright.config.ts --project=chromium --reporter=list tests/e2e/support/e2e.setup.ts",
    "lint": "oxlint src",
    "lint:fix": "oxlint src --fix",
    "format": "oxfmt src",
    "format:check": "oxfmt src --check",
    "typecheck": "vue-tsc --noEmit",
    "ci": "pnpm run lint && pnpm run typecheck && pnpm run test:run"
  },
  "dependencies": {
    "@iconify/vue": "5.0.0",
    "@nuxt/ui": "^4.4.0",
    "@vueuse/core": "14.2.0",
    "@vueuse/motion": "3.0.3",
    "axios": "^1.13.5",
    "jwt-decode": "4.0.0",
    "pinia": "^3.0.4",
    "vue": "^3.5.28",
    "vue-router": "^4.6.4",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@playwright/test": "1.58.2",
    "@tailwindcss/vite": "^4.1.18",
    "@tsconfig/node22": "^22.0.5",
    "@types/jsdom": "27.0.0",
    "@types/node": "^25.2.2",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "jsdom": "^28.0.0",
    "openapi-typescript": "7.12.0",
    "oxfmt": "0.28.0",
    "oxlint": "^1.43.0",
    "rollup-plugin-visualizer": "6.0.5",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "npm:rolldown-vite@^7.3.0",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.4"
  }
}
